#calender
    .col-md-12
      .row
        .col-md-3(style='height:35px;border-right:1px solid rgba(220,220,220,1);')
        .col-md-7
          .row(style='height:35px;background-color:white;border-right:1px solid rgba(220,220,220,1);')
            .col-md-12
              .col-md-1.col-md-offset-5
