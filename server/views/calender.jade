              .col-md-1(style = 'margin-left: 185px')
                button.btn.btn-primary.arrow-left.btn-xs(type='button' data-toggle='', data-target='' id="left" style = 'padding: 0px 4px; margin-top: 4px')
                  span.glyphicon.glyphicon-arrow-left
              .col-md-1(style = 'margin-left: -31px')
