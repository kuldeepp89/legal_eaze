doctype html
html(lang='en')
head
    meta(charset='utf-8')
    title #{title}
    link(href='/public/css/vendor/bootstrap.min.css', rel='stylesheet')
    link(href='/public/css/dashboard.css', rel='stylesheet')
    link(href='/public/css/vendor/jquery-ui.css', rel='stylesheet')  
    link(href='/public/css/vendor/iThing.css', rel='stylesheet')  
    link(href='/public/css/listpage.css', rel='stylesheet')
    script(src='/public/js/vendor/jquery-1.11.1.min.js', type='text/javascript')
    script(src='/public/js/vendor/jquery-ui.js', type='text/javascript')
    script(src='/public/js/vendor/bootstrap.min.js', type='text/javascript')
    script(src='/public/js/vendor/jQDateRangeSlider-min.js', type='text/javascript')
    script(src='/public/js/vendor/bootstrap-datepicker.js', type='text/javascript')
    script(src='/public/js/vendor/bootstrap-filestyle.js', type='text/javascript')
    script(src='/public/js/listpage.js', type='text/javascript')
    script(src='/public/js/userDashboard.js', type='text/javascript')
    script(src='/public/js/homepage.js', type='text/javascript')
   
  body
    .navbar.navbar-inverse.navbar-fixed-top(role='navigation')
      .container-fluid
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          img(src='/public/images/court-img.png', height='60px', width:='width:', 60px='60px')
        .navbar-collapse.collapse
          ul.nav.navbar-nav.navbar-right
            li
              a(href='/adminuserdetail') Users Detail
            li
              a(href='/logout') Logout
    .container-fluid
      .row-fluid
        .span12
          h1
          | User List
        .row-fluid
          .span12
            ul.recent-comments
              a(href = '' style = 'color: gray')
                for user in users
                  li.comment-box(style = 'width: 49%;float: left; margin-left: 10px')
                    .row-fluid
                      .span9
                        .row-fluid
                          .span6
                            strong Name: #{user.profile.name}
                          .span6
                            strong Age: #{user.profile.age}
                          .span6
                            strong Address: #{user.profile.address}
                        .row-fluid
                          .span12
                            strong Email ID: #{user.local.email}        
    script(src='/public/js/jquery-1.10.2.js', type='text/javascript')
    script(src='/public/js/jquery-ui.js', type='text/javascript')
    script(src='/public/js/bootstrap.min.js', type='text/javascript')


